package tp2;

/**
 *
 * @author JPS
 */
public class PileEntiers extends Pile {

    public PileEntiers(){
        super();
    }

    public PileEntiers(Pile p1) {
        super(p1);
    }

    @Override
    public Integer Depiler() throws VideException{
        return (Integer) super.Depiler(); // super de la maman. on caste l'Object en Integer
    }

    public void Empiler(Integer element){
        super.Empiler(element);
    }
}
